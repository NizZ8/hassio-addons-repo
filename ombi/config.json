{
  "name": "Ombi",
  "version": "latest",
  "slug": "ombi_docker",
  "description": "Ombi add-on based on the prebuilt docker image from linuxserver",
  "url": "https://github.com/NizZ8/hassio-addons-repo/ombi",
  "webui": "http://[HOST]:[PORT:3579]",
  "panel_icon": "mdi:emoticon-cool",
  "startup": "services",
  "arch": [
    "aarch64",
    "amd64",
    "armhf"
  ],
  "ports": {
    "3579/tcp": 3579
  },
  "map": [
    "backup:rw",
    "share:rw",
    "ssl"
  ],
  "boot": "auto",
  "environment": {
    "PUID": "0",
    "PGID": "0"
  },
  "options": {},
  "schema": {}
}
